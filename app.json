{
  "expo": {
    "name": "inner",
    "slug": "inner",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "inner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "inner-app",
          "organization": "inner-xh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
